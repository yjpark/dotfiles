{
    "python.pythonPath": "/anaconda3",
    "window.zoomLevel": 0,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/*.meta": true,
        "**/.*.meta": true
    },
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.scrollbar.vertical": "hidden",
    "editor.overviewRulerBorder": false,
    "editor.minimap.enabled": true,
    "editor.minimap.side": "right",
    "editor.minimap.showSlider": "always",
    "editor.minimap.renderCharacters": false,
    "editor.minimap.maxColumn": 80,
    "vim.neovimPath": "C:/Users/yjpark/Applications/Neovim/bin/nvim",
    "vim.enableNeovim": true,
    "vim.disableAnnoyingNeovimMessage": true,
    "vim.useSystemClipboard": true,
    "vim.sneak": true,
    "vim.easymotion": true,
    "vim.easymotionKeys": "aoeuidhtnspyfgcrlqjkxbmwvz,",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors": {
        "normal": ["#8FBCBB", "#434C5E"],
        "insert": "#BF616A",
        "visual": "#B48EAD",
        "visualline": "#B48EAD",
        "visualblock": "#A3BE8C",
        "replace": "#D08770"
    },
    "vim.normalModeKeyBindings": [
        {
            "before":[" "],
            "after":["<Leader>", "<Leader>"]
        }, {
            "before":[";"],
            "after":[":"]
        }, {
            "before":["<Enter>"],
            "after":["i"]
        }, {
            "before":["<Backspace>"],
            "after":["i", "<Backspace>"]
        }, {
            "before":["<Comma>", "b"],
            "after":[],
            "commands": [
                {
                    "command": "workbench.action.toggleSidebarVisibility",
                    "args": []
                }
            ]
        }, {
            "before":["<C-w>"],
            "after":[],
            "commands": [
                {
                    "command": "workbench.action.closeActiveEditor",
                    "args": []
                }
            ]
        }, {
            "before":["<C-n>"],
            "after":[],
            "commands": [
                {
                    "command": ":tabnext",
                    "args": []
                }
            ]
        }, {
            "before":["<C-p>"],
            "after":[],
            "commands": [
                {
                    "command": ":tabprevious",
                    "args": []
                }
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                ">"
            ],
            "commands": [
                {
                    "command": "editor.action.indentLines"
                }
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                {
                    "command": "editor.action.outdentLines"
                }
            ]
        },
    ],
    "editor.wordWrap": "on",
    "[fsharp]": {
        "editor.useTabStops": true
    },
    "workbench.colorCustomizations": {
        "statusBar.background": "#8FBCBB",
        "statusBar.noFolderBackground": "#8FBCBB",
        "statusBar.debuggingBackground": "#8FBCBB",
        "statusBar.foreground": "#434C5E"
    }
}
