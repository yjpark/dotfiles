#!/usr/bin/env bash

install-cargo-tools.libs

cargo install cargo-watch
cargo install fmt
cargo install fix

# seed
cargo install cargo-make
rustup target add wasm32-unknown-unknown

# orbtk
cargo install cargo-node

# Diesel
cargo install diesel_cli

# Tools
cargo install broot
cargo install tokei

cargo install pier
cargo install ytop
cargo install fblog

