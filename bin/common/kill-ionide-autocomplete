#!/bin/bash

/bin/ps -ef | grep --color=no -v "grep" | grep --color=always -G "fsautocomplete.exe"
/bin/ps -ef | grep --color=no -v "grep" | grep --color=no -G "fsautocomplete.exe" | awk '{print $2}' | xargs kill -9
echo "--------------------------------------------------------------------------------"
/bin/ps -ef | grep --color=no -v "grep" | grep --color=always -G "fsautocomplete.exe"

